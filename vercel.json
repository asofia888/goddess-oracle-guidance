{
  "functions": {
    "api/generateMessage.js": {
      "maxDuration": 60
    },
    "api/generateImage.js": {
      "maxDuration": 120
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}